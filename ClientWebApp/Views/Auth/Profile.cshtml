@model IEnumerable<System.Security.Claims.Claim>

<h2>User Profile</h2>

<table>
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Auth/Logout">Logout</a>
